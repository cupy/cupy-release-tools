configs {
  key: "cupy-release-tools"
  value {
    requirement {
      cpu: 16
      gpu: 1
      memory: 104
      disk: 30
    }
    time_limit {
      seconds: 10800
    }
    command: ".pfnci/wheel-linux/main.sh 13.x 3.13"
  }
}
configs {
  key: "cupy-release-tools.linux.rocm"
  value {
    requirement {
      cpu: 8
      memory: 36
      disk: 30
    }
    time_limit {
      seconds: 10800
    }
    command: ".pfnci/wheel-linux/main.sh rocm-7.0 3.12"
    environment_variables { key: "CUPY_RELEASE_SKIP_VERIFY" value: "1" }
  }
}
